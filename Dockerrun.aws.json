{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "springboot",
      "image": "twoosky/docker-springboot",
      "hostname": "springboot",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "twoosky/docker-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["springboot"],
      "memory": 128
    }
  ]
}